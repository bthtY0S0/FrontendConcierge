{
  "expo": {
    "name": "concierge-frontend-classic",
    "slug": "concierge-frontend-classic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "web": {
  "favicon": "./assets/favicon.png",
  "manifest": {
    "name": "Concierge App",
    "short_name": "Concierge",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#4CAF50",
    "icons": [
      {
        "src": "./assets/icon-192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "./assets/icon-512.png",
        "sizes": "512x512",
        "type": "image/png"
      },
      {
        "src": "./assets/icon-180.png",
        "sizes": "180x180",
        "type": "image/png"
      }
    ]
  }
}
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
